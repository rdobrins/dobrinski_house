<div class="images-container">
  <div class="images-content">
    <% if @house_images %>
      <% @house_images.each do |h| %>
        <%= render "house_image", house_image: h %>
      <% end %>
      <%= paginate @house_images %>
    <% else %>
      <span>No Images Present.</span>
    <% end %>
  </div>
</div>
